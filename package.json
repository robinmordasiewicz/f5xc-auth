{
  "name": "@robinmordasiewicz/f5xc-auth",
  "version": "1.4.0",
  "description": "Shared authentication library for F5 Distributed Cloud MCP servers - XDG-compliant profile management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest watch --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:uat": "vitest run tests/uat",
    "test:validate": "node scripts/validate-tests.js",
    "test:ci": "npm run test:coverage && npm run test:validate",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "f5",
    "f5xc",
    "f5-distributed-cloud",
    "authentication",
    "xdg",
    "profile-management",
    "mcp",
    "model-context-protocol"
  ],
  "author": "Robin Mordasiewicz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robinmordasiewicz/f5xc-auth.git"
  },
  "bugs": {
    "url": "https://github.com/robinmordasiewicz/f5xc-auth/issues"
  },
  "homepage": "https://robinmordasiewicz.github.io/f5xc-auth/",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "yaml": "^2.7.0"
  },
  "overrides": {
    "glob": ">=10.5.0",
    "tar": ">=7.5.3",
    "undici": ">=6.23.0",
    "diff": ">=8.0.3",
    "semantic-release": {
      "@semantic-release/npm": "$@semantic-release/npm"
    }
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/node": "^22.10.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "axios-mock-adapter": "^2.1.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "semantic-release": "^24.2.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
